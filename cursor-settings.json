{
  "editor.fontSize": 14,
  "editor.quickSuggestionsDelay": 10,
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.suggestSelection": "first",
  "editor.fontFamily": "JetBrains Mono,Input Mono,Fira Code,Space Mono,Monaco,Menlo,IBM Plex Mono",
  "editor.fontLigatures": false,
  "editor.fontWeight": 600,
  "editor.wordWrapColumn": 100,
  "editor.letterSpacing": 0.2,
  "editor.lineHeight": 1.5,
  "editor.lineNumbers": "interval",
  "editor.inlineSuggest.enabled": true,
  "editor.glyphMargin": true,
  "editor.guides.bracketPairs": "active",
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.renderWhitespace": "boundary",
  "editor.tabSize": 2,
  "editor.comments.insertSpace": true,
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": "always"
  },
  "editor.quickSuggestions": {
    "other": "on",
    "comments": "off",
    "strings": "off"
  },
  "workbench.list.smoothScrolling": true,
  "workbench.editor.closeOnFileDelete": true,
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.editor.enablePreview": false,
  "workbench.iconTheme": "file-icons",
  "workbench.sideBar.location": "left",
  "workbench.activityBar.orientation": "vertical",
  "workbench.colorTheme": "Cursor Dark Midnight",
  "workbench.preferredDarkColorTheme": "Cursor Dark Midnight",
  "workbench.preferredHighContrastColorTheme": "Night Owl Light (No Italics)",
  "window.dialogStyle": "custom",
  "window.titleBarStyle": "custom",
  "window.autoDetectColorScheme": false,
  "window.nativeTabs": true,
  "window.systemColorTheme": "dark",
  "terminal.integrated.fontSize": 12,
  "terminal.integrated.defaultProfile.windows": "Git Bash",
  "terminal.integrated.defaultProfile.osx": "zsh",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.fontWeight": "500",
  "terminal.integrated.persistentSessionReviveProcess": "never",
  "terminal.integrated.tabs.enabled": true,
  "debug.console.fontSize": 12,
  "security.workspace.trust.untrustedFiles": "open",
  "prettier.tabWidth": 2,
  "prettier.printWidth": 150,
  "prettier.semi": true,
  "prettier.useTabs": false,
  "prettier.singleQuote": false,
  "prettier.jsxSingleQuote": true,
  "prettier.bracketSpacing": true,
  "prettier.trailingComma": "none",
  "prettier.arrowParens": "always",
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[less]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "git.autofetch": true,
  "git.confirmSync": false,
  "scm.inputFontSize": 14,
  "scm.diffDecorationsGutterWidth": 2,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "files.autoSave": "afterDelay",
  "markdown.preview.fontSize": 14,
  "explorer.confirmDelete": false,
  "diffEditor.ignoreTrimWhitespace": false,
  "polacode.transparentBackground": true,
  "polacode.backgroundColor": "#f3f3f3",
  "unocss.root": "packages/client",
  "diffEditor.renderSideBySide": false
}
